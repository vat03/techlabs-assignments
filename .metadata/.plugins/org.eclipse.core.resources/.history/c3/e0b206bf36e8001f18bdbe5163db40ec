package com.aurionpro.test;

import java.util.ArrayList;
import java.util.List;

public class ListTest {
	public static void main(String[] args) {
		List<Integer> numbers = new ArrayList<Integer>();
		
		numbers.add(50);
		numbers.add(30);
		numbers.remove(0);
		
		
		System.out.println(numbers);
	}
}
