package com.aurionpro.model;

public class Bike implements Vehicle {
    @Override
    public void start() {
        System.out.println("Bike is starting with a vroom!");
    }

    @Override
    public void stop() {
        System.out.println("Bike is stopping quickly.");
    }

    @Override
    public String fuelType() {
        return "Petrol";
    }
}
